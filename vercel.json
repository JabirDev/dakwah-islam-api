{
    "version": 2,
    "builds": [
        {
            "src": "./**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "./routes/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}